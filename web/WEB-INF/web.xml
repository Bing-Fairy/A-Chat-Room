<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>com.web.servlet.LoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/servlet/loginServlet</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>CkEmailServlet</servlet-name>
        <servlet-class>com.web.servlet.check.CkEmailServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CkEmailServlet</servlet-name>
        <url-pattern>/servlet/ckEmailServlet</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>UpdateLoginPeople</servlet-name>
        <servlet-class>com.web.servlet.UpdateLoginPeople</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UpdateLoginPeople</servlet-name>
        <url-pattern>/servlet/updateLoginPeople</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>SendMyMessage</servlet-name>
        <servlet-class>com.web.servlet.SendMyMessage</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SendMyMessage</servlet-name>
        <url-pattern>/servlet/sendMyMessage</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>LoginOutServlet</servlet-name>
        <servlet-class>com.web.servlet.LoginOutServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginOutServlet</servlet-name>
        <url-pattern>/servlet/loginOutServlet</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>SendAllMessages</servlet-name>
        <servlet-class>com.web.servlet.SendAllMessages</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SendAllMessages</servlet-name>
        <url-pattern>/servlet/sendAllMessages</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>RegisServlet</servlet-name>
        <servlet-class>com.web.servlet.RegisterServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegisServlet</servlet-name>
        <url-pattern>/servlet/regisServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>CkPasswordServlet</servlet-name>
        <servlet-class>com.web.servlet.check.CkPasswordServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CkPasswordServlet</servlet-name>
        <url-pattern>/servlet/ckPasswordServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>CkNameServlet</servlet-name>
        <servlet-class>com.web.servlet.check.CkNameServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CkNameServlet</servlet-name>
        <url-pattern>/servlet/ckNameServlet</url-pattern>
    </servlet-mapping>

</web-app>